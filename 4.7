#include <stdio.h>

int main() {
    float celsius, fahrenheit;

    // รับอุณหภูมิจากผู้ใช้
    printf("User input :\n");
    scanf("%f", &celsius);

    // แปลงจากเซลเซียสเป็นฟาเรนไฮต์
    fahrenheit = (celsius * 9 / 5) + 32;

    // แสดงผลลัพธ์
    printf("%.1f degree Celsius = %.1f degree Fahrenheit\n", celsius, fahrenheit);

    return 0;
}
